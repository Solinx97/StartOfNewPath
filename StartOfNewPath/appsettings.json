{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=Start_Of_New_Path;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "StartOfNewPath": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",
  "TokenSettings": {
    "Issuer": "MySecretIssuer",
    "Audience": "MySecretAudience",
    "AuthScheme": "CustomBearer ",
    "Name": "JWTKey"
  }
}
